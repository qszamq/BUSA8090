mysql> use A1T3Q1;
mysql> create table annotation (gene VARCHAR(7), function VARCHAR(30), metabolism VARCHAR(30));
mysql> INSERT INTO annotation (gene, function, metabolism) VALUES ("alr2938", "iron superoxide dismutase", "Detoxification");
mysql> INSERT INTO annotation (gene, function, metabolism) VALUES ("alr4392", "nitrogen-responsive regulator", "Nitrogen assimilation");
mysql> INSERT INTO annotation (gene, function, metabolism) VALUES ("alr4851", "preprotein translocase subunit", "Protein and peptide secretion");
mysql> INSERT INTO annotation (gene, function, metabolism) VALUES ("alr3395", "adenylosuccinate lyase", "Purine biosynthesis");
mysql> INSERT INTO annotation (gene, function, metabolism) VALUES ("alr1207", "uridylate kinase", "Pyrimidine biosynthesis");
mysql> INSERT INTO annotation (gene, function, metabolism) VALUES ("alr5000", "CTP synthetase", "Pyrimidine biosynthesis");
mysql> INSERT INTO annotation (gene, function, metabolism) VALUES ("all3556", "succinate dehydrogenase", "TCA cycle");
mysql> SELECT*FROM annotation;
